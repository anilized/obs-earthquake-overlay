{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/ws", "destination": "/api/ws-proxy" },
    { "source": "/ws/(.*)", "destination": "/api/ws-proxy" },
    { "source": "/api/ws-proxy", "destination": "/api/ws-proxy" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
